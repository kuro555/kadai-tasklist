<%= form_with(model: @user, local: true) do |f| %>

    <%= f.label :name, 'name' %>
    <%= f.text_field :name %>
    <%= f.label :email, 'Eメール' %>
    <%= f.text_field :email %>
    <%= f.label :password, 'パスワード' %>
    <%= f.password_field :password %>
    <%= f.label :password_confirmation, 'パスワード（確認）' %>
    <%= f.password_field :password_confirmation%>
    
    <%= f.submit '登録' %>

<% end %>